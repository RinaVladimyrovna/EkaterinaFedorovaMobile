<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Web Suite" verbose="1">

    <!-- Please, use given parameter to insert address of desired property file. -->
    <!-- Please, select files from rc/main/resources/properties/ directory -->
    <parameter name="webPropertyFile" value="src/main/resources/properties/webIos.properties"/>

    <test name="web">

        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>

        <packages>
            <package name="mobileWebAppTest"/>
        </packages>

    </test>

</suite>