<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Native Suite" verbose="1">

    <!-- Please, use given parameter to insert address of desired property file. -->
    <!-- Please, select files from rc/main/resources/properties/ directory -->
    <parameter name="nativePropertyFile" value="src/main/resources/properties/native.properties"/>

    <test name="native">

        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>

        <packages>
            <package name="mobileNativeAppTest"/>
        </packages>

    </test>

</suite>